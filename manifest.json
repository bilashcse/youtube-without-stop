  
{
    "manifest_version": 2,
    "name": "youtube pro",
    "description": "Using youtube video without stop/pause popup",
    "version": "0.0.1",
    "content_scripts": [
    {
        "matches": [
            "https://www.youtube.com/*"
          ],
        "js": ["src/main.js"]
    }],
    "permissions": [
        "https://www.youtube.com/*"
        ]
  }